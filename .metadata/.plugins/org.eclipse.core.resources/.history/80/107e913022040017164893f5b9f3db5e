package ast.tipos;

public class CampoRegistro {
	private String nombre;
	private int OFFSET;
	private Tipo tipo;

	public CampoRegistro(String nombre, Tipo tipo) {
		this.nombre = nombre;
		this.tipo = tipo;
	}

	@Override
	public String toString() {
		return "CampoRegistro [nombre=" + nombre + ", OFFSET=" + OFFSET
				+ ", tipo=" + tipo + "]";
	}

}
