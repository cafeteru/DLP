package ast.tipos;

import ast.util.NodoPosicion;

public class TipoCaracter extends NodoPosicion implements Tipo {
	public TipoCaracter(int linea, int columna) {
		super(linea, columna);
	}

	private static TipoCaracter instancia;

	public static TipoCaracter getInstancia(int linea, int columna) {
		if (instancia == null)
			instancia = new TipoCaracter(linea, columna);
		return instancia;
	}

	@Override
	public String toString() {
		return "TipoCaracter []";
	}

}
