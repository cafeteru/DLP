package visitor;

import ast.Programa;
import ast.definiciones.*;
import ast.expresiones.*;

public interface Visitor {
	public void visit(Programa programa);

	public void visit(DefVariable defVariable);

	public void visit(DefFuncion defFuncion);

	public void visit(AccesoArray accesoArray);

	public void visit(AccesoCampo accesoCampo);

	public void visit(Aritmetica aritmetica);

	public void visit(Cast cast);

	public void visit(Comparacion comparacion);

	public void visit(LiteralCaracter literalCaracter);

	public void visit(LiteralEntero literalEntero);

	public void visit(Logica logica);

	public void visit(MenosUnario menosUnario);

	public void visit(Negacion negacion);

	public void visit(Variable variable);
}
