// * Variables globales
int i,n;
char c;
struct {
  int intField;
  double realField;
} record;

// * Funciones
int f(int a, double c) {
  double real;
  real = c;
  i = a;
  return n;
}

// * Programa principal
void main() {
  int i;
  double r;
  write i, f(n,r), record.intField, record.realField;
}

